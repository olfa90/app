<?xml version='1.0' encoding='utf-8'?>
<widget id="com.menufortourist" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidLaunchMode" value="singleTop" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <preference name="Fullscreen" value="true" />
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <feature name="Globalization">
        <param name="android-package" value="org.apache.cordova.globalization.Globalization" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>Menu For TOURIST</name>
    <description>
        An incredible app for turists to find restaurant's menu in theirs home languages.
    </description>
    <author email="mazzoni.developer@gmail.com" href="http://menufortourist.com">
        Gustavo Mazzoni Maia Lopes
    </author>
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.5.0" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="2500" />
    <gap:plugin name="org.apache.cordova.device" />
    <gap:plugin name="org.apache.cordova.device-motion" />
    <gap:plugin name="org.apache.cordova.device-orientation" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.file" />
    <gap:plugin name="org.apache.cordova.file-transfer" />
    <gap:plugin name="org.apache.cordova.geolocation" />
    <gap:plugin name="org.apache.cordova.globalization" />
    <gap:plugin name="org.apache.cordova.inappbrowser" />
    <gap:plugin name="org.apache.cordova.media" />
    <gap:plugin name="org.apache.cordova.network-information" />
    <gap:plugin name="org.apache.cordova.splashscreen" />
    <gap:plugin name="org.apache.cordova.vibration" />
    <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" version="1.2.0" />
    <gap:plugin name="hu.dpal.phonegap.plugins.spinnerdialog" version="0.2.1" />
    <icon src="icon.png" />
    <icon gap:platform="android" gap:qualifier="ldpi" src="res/icon/android/logo-36.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="res/icon/android/logo-48.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="res/icon/android/logo-72.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="res/icon/android/logo-96.png" />
    <icon gap:platform="ios" height="57" src="res/icon/ios/logo-57.png" width="57" />
    <icon gap:platform="ios" height="72" src="res/icon/ios/logo-72.png" width="72" />
    <icon gap:platform="ios" height="114" src="res/icon/ios/logo-114.png" width="114" />
    <icon gap:platform="ios" height="144" src="res/icon/ios/logo-144.png" width="144" />
    <icon gap:platform="winphone" src="res/icon/windows-phone/logo-48.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-ldpi" src="www/res/screen/android/screen-ldpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-hdpi" src="www/res/screen/android/screen-hdpi-portrait.png" />
    <gap:splash gap:platform="android" gap:qualifier="port-xhdpi" src="www/res/screen/android/screen-xhdpi-portrait.png" />
    <gap:splash gap:platform="ios" height="480" src="www/res/screen/ios/Menu4Tourists_SplashPage_320x480px.jpg" width="320" />
    <gap:splash gap:platform="ios" height="960" src="www/res/screen/ios/Menu4Tourists_SplashPage_640x960.jpg" width="640" />
    <gap:splash gap:platform="ios" height="1136" src="www/res/screen/ios/Menu4Tourists_SplashPage_640x1136px.jpg" width="640" />
    <gap:splash gap:platform="ios" height="1024" src="www/res/screen/ios/Menu4Tourists_SplashPage_768x1024px.jpg" width="768" />
    <gap:splash gap:platform="ios" height="2048" src="www/res/screen/ios/Menu4Tourists_SplashPage_1536x2048px.jpg" width="1536" />
    <access origin="*.google.com" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="*" />
</widget>
